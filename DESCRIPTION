Package: meteorologia
Title: Analisis de Estaciones Meteorologicas y Datos Ambientales
Version: 0.0.0.9000
Authors@R: c(
  person("Bianca", "Rossi", email = "biancarossiscaglia1@gmail.com", role = c("aut", "cre")),
